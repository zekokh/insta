<div class="container-fluid">
  <div class="row">
    <%= render 'patrial/navbar' %>
    <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
      <%= render('patrial/sidebar', menu: {active: "заблокированные"}) %>
      <div class="col-lg-12">

        <% if @members_false.length > 0 %>
            <table class="table table-hover">
              <thead>
              <tr>
                <th></th>
                <th></th>
                <th></th>
              </tr>
              </thead>
              <tbody>
              <% @members_false.each do |member| %>
                  <tr> <td><%= image_tag member.photo_url, height: '32', width: '32' %></td></tr>
                  <tr>
                    <td>
                    <% if member.name.length > 0 %>
                        <%= member.name %>
                    <% else %>
                        Имя не указанно
                    <% end %>
                    <%= member.nickname %>
                    Лайки: <%= member.likes %>
                    <%= Time.at(member.publication_date.to_i).to_formatted_s(:db) %>
                    </td>
                  </tr>
              <% end %>
              </tbody>
            </table>
        <% end %>
      </div>
    </div>
  </div>
</div>